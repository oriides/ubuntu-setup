<filteroptions>
    <ruleset>
        <rule enabled="true" grouping="all" source="incoming">
            <title>7digital</title>
            <partset>
                <part name="to">
                    <value name="recipient-type" type="option" value="contains" />
                    <value name="recipient" type="address" allow-empty="false">
                        <address>REDACTED+7d@gmail.com</address>
                    </value>
                </part>
            </partset>
            <actionset>
                <part name="label">
                    <value name="label" type="option" value="via_7digital" />
                </part>
            </actionset>
        </rule>
